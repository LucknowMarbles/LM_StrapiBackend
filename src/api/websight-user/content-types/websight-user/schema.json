{
  "kind": "collectionType",
  "collectionName": "websight_users",
  "info": {
    "singularName": "websight-user",
    "pluralName": "websight-users",
    "displayName": "WebsightUser"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Password": {
      "type": "password"
    },
    "UserType": {
      "type": "enumeration",
      "enum": [
        "Customer",
        "Vender",
        "Accounts",
        "Admin",
        "Sales and Marketing",
        "HR",
        "Local Truck Driver",
        "Big Truck Driver",
        "Support",
        "Contractors"
      ]
    },
    "contact": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::contact.contact"
    },
    "enquiries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enquiry.enquiry",
      "mappedBy": "websight_user"
    }
  }
}
